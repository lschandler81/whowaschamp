{
  "name": "birthday-champion-finder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:onthisday": "node scripts/build_on_this_day.mjs",
    "build:ufc-events": "node scripts/build_ufc_events.mjs",
    "build:ufc-reigns": "node scripts/build_ufc_reigns.mjs",
    "build": "npm run build:onthisday && npx prisma generate && next build",
    "build:netlify": "npx prisma db push --accept-data-loss && npx tsx scripts/update-all-recent-data.ts && mkdir -p netlify/functions && cp dev.db netlify/functions/ && npm run build",
    "dev": "next dev",
    "start": "next start",
    "postbuild": "next-sitemap",
    "lint": "next lint",
    "clean": "rm -rf .next",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:weekly": "tsx scripts/test-weekly-rotation.ts",
    "refresh:weekly": "curl -X POST -H \"Authorization: Bearer $WEEKLY_REFRESH_TOKEN\" http://localhost:3000/api/refresh/weekly",
    "scrape": "node scripts/build_reigns.mjs",
    "scrape-all": "node scripts/build_reigns.mjs && node scripts/build_more_titles.mjs",
    "validate-coverage": "node scripts/validate_coverage.mjs",
    "validate-data": "node -e 'require(\"./lib/schema\").parseChampionshipData(require(\"./public/data/wwe_championship_reigns.json\"))'",
    "postinstall": "prisma generate",
    "prebuild": "prisma generate",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate deploy",
    "db:seed": "npx tsx scripts/etl/seed.ts",
    "etl:ufc": "npx tsx scripts/etl/backfill_ufc_new.ts",
    "etl:wwe": "npx tsx scripts/etl/backfill_wwe_new.ts",
    "etl:all": "npm run etl:ufc && npm run etl:wwe",
    "etl:health": "./scripts/deploy/run-etl.sh health",
    "deploy:migrate": "./scripts/deploy/migrate-db.sh",
    "deploy:build": "docker build -f Dockerfile.prod -t whowaschamp:latest .",
    "deploy:up": "docker-compose -f docker-compose.prod.yml up -d",
    "deploy:down": "docker-compose -f docker-compose.prod.yml down",
    "validate:apis": "node scripts/validate_apis.mjs",
    "generate-ppv-data": "npx tsx scripts/generate-ppv-data.ts",
    "generate-ufc-data": "npx tsx scripts/generate-ufc-data.ts",
    "generate-wwe-data": "npx tsx scripts/generate-wwe-data.ts",
    "generate-split-data": "npm run generate-ufc-data && npm run generate-wwe-data",
    "create-complete-wwe-dataset": "npx tsx scripts/create-complete-wwe-dataset.ts",
    "scrape-wwe-wikipedia": "npx tsx scripts/scrape-complete-wwe-wikipedia.ts",
    "add-remaining-wwe-events": "npx tsx scripts/add-remaining-wwe-events.ts",
    "add-final-wwe-events": "npx tsx scripts/add-final-wwe-events.ts",
    "add-ppv": "node scripts/add-ppv.js",
    "update-static-data": "npm run generate-ppv-data && git add public/data/ && git commit -m 'Update static PPV data' && git push"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@netlify/functions": "^4.2.5",
    "@next/swc-wasm-nodejs": "13.5.1",
    "@prisma/client": "^6.16.1",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-context-menu": "^2.2.1",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-hover-card": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@types/node": "20.19.0",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "10.4.15",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "embla-carousel-react": "^8.3.0",
    "eslint": "8.49.0",
    "eslint-config-next": "13.5.1",
    "input-otp": "^1.2.4",
    "lucide-react": "^0.446.0",
    "next": "13.5.1",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.3.0",
    "node-html-parser": "^7.0.1",
    "postcss": "8.4.30",
    "prisma": "^6.16.1",
    "react": "18.2.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.53.0",
    "react-resizable-panels": "^2.1.3",
    "recharts": "^2.12.7",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.2.2",
    "uuid": "^13.0.0",
    "vaul": "^0.9.9",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cheerio": "^1.0.0",
    "cheerio": "^1.1.2",
    "node-fetch": "^3.3.2",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "@types/node": "20.19.0"
  }
}
